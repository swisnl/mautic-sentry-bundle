{
    "name": "swisnl/mautic-sentry-bundle",
    "description": "Sentry integration for Mautic",
    "type": "mautic-plugin",
    "keywords": [
        "mautic",
        "plugin",
        "integration",
        "sentry",
        "swisnl",
        "mautic-sentry-bundle"
    ],
    "homepage": "https://github.com/swisnl/mautic-sentry-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Biesenbeek",
            "email": "tbiesenbeek@swis.nl",
            "homepage": "https://www.swis.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "mautic/core-lib": "^5.0",
        "sentry/sentry": "^4.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit" : "^9.5"
    },
    "scripts": {
        "check-style": "php-cs-fixer fix --dry-run -v",
        "fix-style": "php-cs-fixer fix"
    },
    "extra": {
        "install-directory-name": "MauticSentryBundle",
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
